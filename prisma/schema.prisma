generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model urls {
  id          Int       @id @default(autoincrement()) 
  url         String
  shortCode    String    @unique
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @default(now())
  clicks      Int       @default(0)
  expiresAt   DateTime?
}
